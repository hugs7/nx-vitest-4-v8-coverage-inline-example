{
  "name": "vite-project-ui",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "ui/src",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/ui",
        "configFile": "ui/vite.config.ts",
        "tsConfig": "ui/tsconfig.app.json"
      }
    },
    "dev": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "port": 3000,
        "configFile": "ui/vite.config.ts",
        "buildTarget": "vite-project-ui:build",
        "cwd": "ui"
      }
    },
    "test": {
      "executor": "@nx/vitest:test",
      "outputs": ["{options.coverage.outputDir}"]
    }
  },
  "tags": []
}
